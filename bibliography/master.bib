@article{Haas:2010,
	Affiliation = {Vienna Univ Technol, Inst Mat Chem, A-1060 Vienna, Austria},
	Author = {Haas, Philipp and Tran, Fabien and Blaha, Peter and Pedroza, Luana S. and da Silva, Antonio J. R. and Odashima, Mariana M. and Capelle, Klaus},
	Date-Added = {2011-12-19 18:14:30 +0000},
